#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:7
msgid "Maximum time (in minutes)"
msgstr "Maximum timer value (in minutes)"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:8
msgid ""
"Set maximum selectable time of the slider (in minutes). Use only values "
"greater zero."
msgstr ""
"Set maximum selectable time of the slider (in minutes). Use only values "
"greater zero."

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:13
msgid "Slider position"
msgstr "Slider position"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:14
msgid ""
"Set slider position as percent of the maximum time. Must be in range 0 and "
"100."
msgstr ""
"Set slider position as percent of the maximum time. Must be in range 0 and "
"100."

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:19
#: templates/pref-window-gtk4.ui:91 templates/pref-window.ui:133
msgid "Show settings button"
msgstr "Show settings button"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:20
msgid "Show/hide settings button in widget."
msgstr "Show/hide settings button in widget."

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:25
#: templates/pref-window-gtk4.ui:112 templates/pref-window.ui:169
msgid "Root mode"
msgstr "Root mode"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:26
msgid ""
"Set root mode on/off. In root mode powering off is done via 'pkexec' and "
"'shutdown' terminal command."
msgstr ""
"Set root mode on/off. In root mode powering off is done via 'pkexec' and "
"'shutdown' terminal command."

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:31
#: templates/pref-window-gtk4.ui:132 templates/pref-window.ui:204
msgid "Use suspend"
msgstr "Use suspend"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:32
msgid "Use suspend instead of shutdown command."
msgstr "Use suspend instead of shutdown command."

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:37
#: templates/pref-window-gtk4.ui:151 templates/pref-window.ui:231
msgid "Check command"
msgstr "Check command"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:38
msgid ""
"Run command before shutdown command. Proceed with shutdown only if check "
"command succeedes."
msgstr ""
"Run command before shutdown command. Proceed with shutdown only if check "
"command succeedes."

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:43
#: templates/pref-window-gtk4.ui:170 templates/pref-window.ui:272
msgid "Enable check command"
msgstr "Enable check command"

#: schemas/org.gnome.shell.extensions.shutdowntimer-neumann.gschema.xml:44
msgid "Check command is skipped if disabled."
msgstr "Check command is skipped if disabled."

#: templates/pref-window-gtk4.ui:43 templates/pref-window.ui:56
msgid "Maximum timer value (in minutes)"
msgstr "Maximum timer value (in minutes)"

#: templates/pref-window-gtk4.ui:71 templates/pref-window.ui:98
msgid "Slider position (in &amp; from 0 to 100)"
msgstr "Slider position (in % from 0 to 100)"

#: templates/pref-window-gtk4.ui:111 templates/pref-window.ui:168
msgid ""
"uses 'pkexec shutdown' command, no interruption of timer, but needs root "
"password"
msgstr ""
"uses 'pkexec shutdown' command, no interruption of timer, but needs root "
"password"

#: templates/pref-window-gtk4.ui:149 templates/pref-window.ui:229
msgid ""
"Run check command after timer runs out. Only proceeds if check command "
"terminates successfully."
msgstr ""
"Run check command after timer runs out. Only proceeds if check command "
"terminates successfully."

